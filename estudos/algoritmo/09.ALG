algoritmo "creuza01"
var
      ano_atual, ano_nascimento, idade: Inteiro
      
inicio

      Escreval ("Em que ano nos estamos?")
      Leia (ano_atual)
      Escreval ("Em que ano eu nasci?")
      Leia (ano_nascimento)
      idade <- ano_atual - ano_nascimento
      Escreval ("Minha idade é ", idade, " anos")
      


fimalgoritmo